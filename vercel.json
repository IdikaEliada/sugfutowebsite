{
  "rewrites": [
    { "source": "/",               "destination": "/index.html" },
    { "source": "/home",           "destination": "/index.html" },
    { "source": "/about",          "destination": "/about-us.html" },
    { "source": "/social",        "destination": "/socials.html" },
    { "source": "/contact",
      "destination": "/contact-us.html" },
    { "source": "/resources/:path*.html",
      "destination": "/resources/:path*" },
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    }
  ],
  "redirects": [
  {
    "source": "/resources/:path*.html",
    "destination": "/resources/:path*",
    "permanent": true
  },
  {
    "source": "/:path*.html",
    "destination": "/:path*",
    "permanent": true
  }
],
  "cleanUrls": true,
  "trailingSlash": false
}